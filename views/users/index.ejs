<%- include('../partials/header') %>
<%if (user) {%>
 
<div id='year-title'>
    <h1><%= title %></h1>
    </div>
    <div id="media-list-div">
      <table id="media-list">
        <thead>
          <th>Picture</th>
          <th>Date Viewed</th>
          <th>Title</th>
          <th>Creator</th>
          <th>Rating</th>
        </thead>
        <tbody>
          <%user.viewings.forEach(function(v){%>  
              <tr>
                <td>
                  <a href="/users/<%=v.mediaType%>/<%=v.apiId%>">
                    <img src="<%=v.coverImage%>" width='100px'>
                  </a>
                </td>
                <td><%=v.viewDate%></td>
                <td><%=v.title%></td>
                <td><%=v.creator%></td>
                <td><%=v.rating%>/5</td>
            </tr>
            <%})%>
        </tbody>

      </table>
    </div>

    <%}%>
<%- include('../partials/footer') %>