<%- include('../partials/header') %>
    <div id='year-title'>
    <h1><%= title %></h1>
    </div>
    <div id="media-list-div">
      <table id="media-list">
        <thead>
          <th>Picture</th>
          <th>Date Viewed</th>
          <th>Title</th>
          <th>Creator</th>
        </thead>
        <tbody>
          <%if (user) {%>
          <tr>
            <%user.viewings.forEach(function(v){%>
              <td>
                <a href="/users/<%=v.mediaType%>/<%=v.apiId%>">
                  <button><%=v.apiId%></button>
                </a>
              </td>
              <td><%=v.viewDate%></td>
              <td><%=v.title%></td>
              <td><%=v.creator%></td>
            <%})%>
          </tr>
          <%}%>
        </tbody>
      </table>
    </div>
<%- include('../partials/footer') %>